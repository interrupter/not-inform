{
    "name": "not-inform",
    "version": "0.1.1",
    "description": "not-* family inform interface for information supply/deploy",
    "main": "./src/index.js",
    "scripts": {
        "test": "./node_modules/.bin/mocha --reporter spec",
        "pretest": "./node_modules/.bin/eslint ./src/**/**",
        "lint": "./node_modules/.bin/eslint ./src/**/**",
        "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
        "cover": "istanbul cover _mocha $(find ./test -name \"*.js\" -not -path \"./node_modules/*\")"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/interrupter/not-inform.git"
    },
    "keywords": [
        "not-bulma",
        "not-node",
        "inform"
    ],
    "author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/interrupter/not-inform/issues"
    },
    "homepage": "https://github.com/interrupter/not-inform#readme",
    "dependencies": {
        "not-config": "*",
        "not-log": "*",
        "not-path": "*"
    },
    "devDependencies": {
        "chai": "*",
        "chai-as-promised": "*",
        "eslint": "*",
        "ink-docstrap": "^1.3.2",
        "istanbul": "*",
        "jsdoc": "^3.6.4",
        "mocha": "*",
        "not-bulma": "^1.2.4",
        "not-inform-rule-tag": "0.0.4",
        "not-inform-sink-email": "0.0.9"
