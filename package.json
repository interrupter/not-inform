    "bugs": {
        "url": "https://github.com/interrupter/not-inform/issues"
    },
    "homepage": "https://github.com/interrupter/not-inform#readme",
    "dependencies": {
        "not-path": "*",
        "not-log": "*",
        "not-config": "*"
    },
    "devDependencies": {
        "chai": "*",
        "chai-as-promised": "*",
        "eslint": "*",
        "ink-docstrap": "^1.3.2",
        "istanbul": "*",
        "jsdoc": "^3.5.5",
        "mocha": "*",
        "not-inform-rule-tag": "*",
        "not-inform-sink-email": "*"
    }
